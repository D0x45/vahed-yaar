{
    "compilerOptions": {
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "module": "es6",
        "target": "ES2022",
        "jsx": "react",
        "allowJs": false,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "bundler",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noFallthroughCasesInSwitch": true
    },
    "include": ["./src/*.ts"]
}